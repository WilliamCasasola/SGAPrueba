@model IEnumerable<SGA.Models.EstudianteParaFactura>

@{
    ViewBag.Title = "Dtalles";
}

<h2>Detalles</h2>

<p>
    @Html.ActionLink("Crear Nuevo", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Estudiante.Id)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Estudiante.Id)
        </td>
        <td>
            @Html.ActionLink("Editar", "Edit", new { id = item.Id }) |
            @Html.ActionLink("Detalles", "Details", new { id = item.Id }) |
            @Html.ActionLink("Deshabilitar", "Delete", new { id = item.Id })
        </td>
    </tr>
}

</table>
